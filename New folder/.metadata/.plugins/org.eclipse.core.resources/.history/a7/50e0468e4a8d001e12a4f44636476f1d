package com;

public class TypeCasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		byte a = 12;
		short b = a;
		System.out.println( a);
		System.out.println( b);
		
		int c = 15;
		long d = (int)c;
		
		System.out.println( c);
		System.out.println( d);
		
	

	}

}
